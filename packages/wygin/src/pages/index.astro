---
import Render from "astro-mdoc/src/components/Render.astro";
import { Config, Options, AcfComponents } from 'astro-mdoc/src/modules.js';
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />
		<title>Astro</title>
	</head>
	<body>
		<h1>Astro</h1>
		<p set:text={JSON.stringify(Config)}></p>
		<p set:text={JSON.stringify(Options)}></p>
		<p set:text={JSON.stringify(AcfComponents)}></p>
		<Render body=`{% AccordionItem heading="Heading" content="content" %}
		hiiiii hello world
		{% /AccordionItem %}
		`>HIIII</Render>
	</body>
</html>
